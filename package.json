{
  "name": "spanwright",
  "version": "2.5.2",
  "description": "CLI tool to generate Cloud Spanner E2E testing framework projects with Go database tools and Playwright browser automation",
  "main": "dist/index.js",
  "bin": {
    "spanwright": "dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CLAUDE.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "SECURITY.md"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "pnpm run e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "test:all": "pnpm run test:unit && pnpm run e2e",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "prepare": "pnpm run build",
    "e2e": "./scripts/run-e2e-test.sh",
    "update-go": "npx ts-node scripts/update-go-version.ts",
    "update-go-version": "npx ts-node scripts/update-go-version.ts",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier",
    "fix:eslint": "pnpm run lint:eslint -- --fix",
    "fix:prettier": "pnpm run lint:prettier -- --write",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier"
  },
  "keywords": [
    "spanner",
    "e2e",
    "testing",
    "database",
    "playwright",
    "cloud-spanner",
    "emulator"
  ],
  "author": "nu0ma",
  "license": "MIT",
  "engines": {
    "node": ">=22.15.1",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nu0ma/spanwright.git"
  },
  "homepage": "https://github.com/nu0ma/spanwright#readme",
  "bugs": {
    "url": "https://github.com/nu0ma/spanwright/issues"
  },
  "preferGlobal": true,
  "dependencies": {
    "@types/node": "^24.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
